        <h2>Skrypty</h2>

        <button onclick="ShowText()">Otwórz okienko</button>
        <script>
            function ShowText() {
                var text;
                var input = prompt("Wpisz coś:");
                if (input == null) {
                    text = "Anulowano";
                } else {
                    text = "Wpisałeś " + input;;
                }
                document.getElementById("demo").innerHTML = text;
            }
        </script>

        <p id="demo"></p>

       

